<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="45" vgap="45">
    <TableView fx:id="tableView"

               GridPane.columnIndex="0"
               GridPane.columnSpan="1"
               GridPane.hgrow="ALWAYS"
               GridPane.vgrow="ALWAYS"
               GridPane.rowIndex="0">
        <columns>
            <TableColumn fx:id="filename" text="File"/>
            <TableColumn fx:id="actualClass" text="Actual Class"/>
            <TableColumn fx:id="spamProbability" text="Spam Probability"/>
        </columns>
    </TableView>
    <Label text="Accuracy : ">
    </Label>
    <!-- Add Name Text Field -->
    <TextField fx:id="accurText"/>

    <Label text="Precision : " >
    </Label>
    <!-- Add Name Text Field -->
    <TextField fx:id="precText"/>
</GridPane>
