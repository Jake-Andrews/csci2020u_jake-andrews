<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>


<GridPane fx:controller="sample.Controller"
          xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="45" vgap="45">
    <TableView fx:id="tableView"

               GridPane.columnIndex="0"
               GridPane.columnSpan="1"
               GridPane.hgrow="ALWAYS"
               GridPane.vgrow="ALWAYS"
               GridPane.rowIndex="0">
        <columns>
            <TableColumn fx:id="studentID" text="SID"/>
            <TableColumn fx:id="assignmentMark" text="Assignments"/>
            <TableColumn fx:id="midtermMark" text="Midterm"/>
            <TableColumn fx:id="finalExamMark" text="Final Exam"/>
            <TableColumn fx:id="finalMark" text="Final Mark"/>
            <TableColumn fx:id="letterGrade" text="Letter Grade"/>
        </columns>
    </TableView>
</GridPane>